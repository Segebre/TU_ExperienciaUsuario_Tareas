<h2>Please fill the form below: </h2>
<%= render partial: "shared/errors", locals: {obj: membership} %>
<%= form_for(membership) do |f| %>
<p>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
</p>
<p>
  <%= f.submit "Save" %>
  <%= link_to "Cancel", memberships_path %>
</p>
<% end %>